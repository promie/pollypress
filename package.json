{
  "name": "pollypress",
  "version": "0.1.0",
  "bin": {
    "pollypress": "bin/pollypress.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "dev": "cd frontend && npm run dev",
    "deploy": "cd frontend && npm run build && cd .. && AWS_PROFILE=promie npm run build && AWS_PROFILE=promie npx cdk deploy --all",
    "deploy:infra": "AWS_PROFILE=promie npm run build && AWS_PROFILE=promie npx cdk deploy",
    "diff": "AWS_PROFILE=promie npx cdk diff",
    "synth": "AWS_PROFILE=promie npx cdk synth",
    "destroy": "AWS_PROFILE=promie npx cdk destroy"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.158",
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "aws-cdk": "2.1031.2",
    "dotenv": "^17.2.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.28.1",
    "@aws-sdk/client-polly": "^3.932.0",
    "@aws-sdk/client-s3": "^3.932.0",
    "@aws-sdk/s3-request-presigner": "^3.932.0",
    "aws-cdk-lib": "2.215.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21",
    "zod": "^4.1.12"
  }
}
